                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/epiphanyWertherView" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <Button
            android:hint="@string/coon_extort_meridional_sect"
            android:id="@+id/shadflowerView"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/repetition_hollandaise_height"
            android:layout_marginTop="@dimen/pulse_show_size"
            android:layout_width="wrap_content"
            android:text="@string/wineskin_ingenuity_cowpony_whelp"
