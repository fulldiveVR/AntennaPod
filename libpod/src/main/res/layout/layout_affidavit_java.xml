            <EditText
                android:hint="@string/reputation_spatlum_sunlight_mutatis"
                android:id="@+id/politickingShunView"
                android:inputType="textVisiblePassword"
                android:layout_height="wrap_content"
                android:layout_width="0dp"
                android:text="@string/alexis_literary_immeasurable_seminar"
                android:textColor="@color/genera_incomparable"
                android:textSize="@dimen/kevin_gujarat_textsize"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/biminiView" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </LinearLayout>

    <AutoCompleteTextView
        android:background="@color/biddy_midmorn_mooch"
        android:hint="@string/menhaden_hair_threadbare_lissajous"
        android:id="@+id/rutileView"
        android:layout_height="wrap_content"
